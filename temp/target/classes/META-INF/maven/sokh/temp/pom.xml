<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>sokh</groupId>
  <artifactId>temp</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>onlineshop</name>
  
  <properties>
		<spring.version>5.1.8.RELEASE</spring.version>
		<springsecurity.version>5.1.5.RELEASE</springsecurity.version>
		<mybatis.version>3.5.2</mybatis.version>
		<mybatisspring.version>2.0.2</mybatisspring.version>
		<servlet.version>4.0.1</servlet.version>
		<mysqlconn.version>8.0.16</mysqlconn.version>
		<dbcp2.version>2.6.0</dbcp2.version>
		<junitplatform.version>1.5.0</junitplatform.version>
		<junit.version>5.5.0</junit.version>
		<hamcrest.version>2.1</hamcrest.version>
		<jackson.version>2.9.9</jackson.version>
		<log4j2.version>2.12.0</log4j2.version>
		<aspectjweaver.version>1.9.4</aspectjweaver.version>
		<javaxannotation.version>1.3.2</javaxannotation.version>
	</properties>
	<dependencies>
		<!-- 0. Servlet接口库，服务器会提供， 编译、测试、运行需要，不会被打包 -->
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<version>${servlet.version}</version>
			<scope>provided</scope>
		</dependency>
		<!-- 1.Spring核心容器 -->
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-context</artifactId>
			<version>${spring.version}</version>
		</dependency>
		<!-- 2.Spring MVC 容器 -->
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-webmvc</artifactId>
			<version>${spring.version}</version>
		</dependency>
		<!-- 3. 数据操作相关包 -->
		<!-- 3. 1 Spring数据操作封装包 -->
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-orm</artifactId>
			<version>${spring.version}</version>
		</dependency>
		<!-- 3. 2 MyBatis -->
		<dependency>
			<groupId>org.mybatis</groupId>
			<artifactId>mybatis</artifactId>
			<version>${mybatis.version}</version>
		</dependency>
		<!-- 3.3 Mybatis-Spring整合包 -->
		<dependency>
			<groupId>org.mybatis</groupId>
			<artifactId>mybatis-spring</artifactId>
			<version>${mybatisspring.version}</version>
		</dependency>
		
	</dependencies>
	
</project>